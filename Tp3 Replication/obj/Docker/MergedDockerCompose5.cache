name: dockercompose7993087582678462358
services:
  database:
    container_name: sqlserver
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Admin123456
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
  tp1apigateway:
    build:
      context: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway
      dockerfile: Tp1ApiGateway/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8081"
    image: tp1apigateway
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 8081
        published: "8081"
        protocol: tcp
  tp1usercontrollmanager:
    build:
      context: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway
      dockerfile: Tp1UserControllManager/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8000"
    image: tp1usercontrollmanager
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 8000
        protocol: tcp
networks:
  my_network:
    name: dockercompose7993087582678462358_my_network
    driver: bridge
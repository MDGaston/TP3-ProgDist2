name: dockercompose7993087582678462358
services:
  blacklistapi:
    build:
      context: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway
      dockerfile: BlackListAPI/Dockerfile
    command:
      - sh
      - -c
      - chmod -R 777 /app/Logs/blacklist && dotnet BlackListAPI.dll
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8003"
    image: blacklistapi
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 8003
        published: "8003"
        protocol: tcp
    volumes:
      - type: bind
        source: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway\Logs\blacklist
        target: /app/Logs/blacklist
        bind:
          create_host_path: true
  database:
    container_name: sqlserver
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_AGENT_ENABLED: "true"
      SA_PASSWORD: Admin123456
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 1433
        published: "1451"
        protocol: tcp
  database_replica:
    container_name: sqlserver_replica
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_AGENT_ENABLED: "true"
      MSSQL_PID: Developer
      MSSQL_REPLICATION_MODE: snapshot
      MSSQL_REPLICATION_TYPE: continuous
      MSSQL_SOURCE_DB: Users db
      MSSQL_SOURCE_SERVER: sqlserver
      SA_PASSWORD: Admin123456
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 1433
        published: "1452"
        protocol: tcp
  rabbitmq:
    environment:
      RABBITMQ_DEFAULT_PASS: password
      RABBITMQ_DEFAULT_USER: user
    image: rabbitmq:3-management
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 5672
        published: "5672"
        protocol: tcp
      - mode: ingress
        target: 15672
        published: "15672"
        protocol: tcp
  tp1apigateway:
    build:
      context: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway
      dockerfile: Tp1ApiGateway/Dockerfile
    command:
      - sh
      - -c
      - chmod -R 777 /app/Logs && dotnet Tp1ApiGateway.dll
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
    image: tp1apigateway
    networks:
      my_network:
        ipv4_address: 172.16.238.10
    ports:
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
    volumes:
      - type: bind
        source: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway\Tp1ApiGateway\appsettings.json
        target: /app/appsettings.json
        bind:
          create_host_path: true
      - type: bind
        source: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway\Logs\tp1apigateway
        target: /app/Logs
        bind:
          create_host_path: true
  tp1usercontrollmanager:
    build:
      context: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway
      dockerfile: Tp1UserControllManager/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8000"
    image: tp1usercontrollmanager
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 8000
        published: "8000"
        protocol: tcp
  tp2Trackingservice:
    build:
      context: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway
      dockerfile: TrackingService/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8001"
    image: tp2trackingservice
    networks:
      my_network: null
    ports:
      - mode: ingress
        target: 8001
        published: "8001"
        protocol: tcp
  trackingConsumer:
    build:
      context: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway
      dockerfile: TrackingConsumer/Dockerfile
    command:
      - sh
      - -c
      - chmod -R 777 /app/trackingLog && sleep 30 && dotnet TrackingConsumer.dll
    depends_on:
      rabbitmq:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8002"
    image: trackingconsumer
    networks:
      my_network:
        ipv4_address: 172.16.238.20
    ports:
      - mode: ingress
        target: 8002
        published: "8002"
        protocol: tcp
    volumes:
      - type: bind
        source: G:\Proyectos de programacion\TP1 ProgDist2\MainTp\Tp1ApiGateway\Logs\trackingConsumer
        target: /app/trackingLog
        bind:
          create_host_path: true
networks:
  my_network:
    name: dockercompose7993087582678462358_my_network
    driver: bridge
    ipam:
      config:
        - subnet: 172.16.238.0/24